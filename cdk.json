{
  "app": "npx ts-node bin/cdk-test-github-action.ts",
  "requireApproval": "never",
  "context": {
    "@aws-cdk/core:enableStackNameDuplicates": "true",
    "aws-cdk:enableDiffNoFail": "true",
    "dev": {
        "stage": "develop",
        "lambda": {
            "defaultMemorySize": 256,
            "defaultTimeOut": 90,
            "logLevel": "DEBUG"
        }
    }
  }
}